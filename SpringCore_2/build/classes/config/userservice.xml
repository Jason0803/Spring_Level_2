<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user01" class="spring.service.domain.User">
        <property name="userId" value="Jason"></property>
        <property name="age" value="24"></property>
    </bean>

    <bean id="password" class="java.lang.String">
        <constructor-arg value="7777"></constructor-arg>
    </bean>
    
    <bean id="user02" class="spring.service.domain.User">
        <property name="userId" value="Sunny"></property>
        <property name="password" value="1205"></property>
        <property name="age" value="28"></property>
    </bean>
    
    <bean id="user03" class="spring.service.domain.User"></bean>
    
    <bean id="user04" class="spring.service.domain.User">
        <constructor-arg value="04"></constructor-arg>
        <constructor-arg value="user04"></constructor-arg>
    </bean>
    
        <bean id="user05" class="spring.service.domain.User">
        <constructor-arg value="05" index="0"></constructor-arg>
        <constructor-arg value="user05" index="1"></constructor-arg>
        <constructor-arg value="0505" index="2"></constructor-arg>
    </bean>
    
</beans>
